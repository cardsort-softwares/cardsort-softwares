{
  "projectType": "application",
  "root": "functions",
  "sourceRoot": "functions/src",
  "targets": {
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npm install firebase-tools",
          "firebase deploy --only functions --config=firebase.website.json"
        ],
        "parallel": false
      }
  }
}
